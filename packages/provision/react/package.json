{
  "name": "@request-kit/provision-react",
  "version": "0.3.0",
  "description": "react high order component API to request and provide data. Not for direct usage, use @request-kit/react-redux instead",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "build": "rollup -c --environment PACKAGE_NAME:$npm_package_name",
    "build:typings": "tsc",
    "watch": "tsc -w & rollup -c -w --environment PACKAGE_NAME:$npm_package_name",
    "prepare": "npm run clean && npm run build:typings && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gleb-lobastov/request-kit.git"
  },
  "keywords": [
    "react",
    "request",
    "provision",
    "data",
    "hoc"
  ],
  "author": "Gleb Lobastov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gleb-lobastov/request-kit/issues"
  },
  "homepage": "https://github.com/gleb-lobastov/request-kit#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "deep-equal": "^1.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-enzyme": "^7.1.0"
  }
}
