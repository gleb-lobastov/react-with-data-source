{
  "name": "@request-kit/react-redux",
  "version": "0.2.0",
  "description": "react high order component, bound with redux, to process request and provide data to component",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "build": "rollup -c --environment PACKAGE_NAME:$npm_package_name",
    "watch": "rollup -c -w --environment PACKAGE_NAME:$npm_package_name",
    "prepare": "npm run clean && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gleb-lobastov/request-kit.git"
  },
  "keywords": [
    "redux",
    "request",
    "data",
    "state"
  ],
  "author": "Gleb Lobastov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gleb-lobastov/request-kit/issues"
  },
  "homepage": "https://github.com/gleb-lobastov/request-kit#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "glob": "^7.1.2",
    "jest-enzyme": "^6.0.1",
    "promise.prototype.finally": "^3.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "redux-mock-store": "^1.5.1"
  },
  "dependencies": {
    "@request-kit/controller-redux": "^0.2.0",
    "@request-kit/distribute-reducer": "^0.2.0",
    "@request-kit/provision-react": "^0.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "shallowequal": "^1.0.2"
  },
  "jest": {
    "globals": {
      "IS_PRODUCTION_ENV": false
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./test/setupEnzyme.js",
      "./test/promiseShim.js"
    ]
  }
}
