{
  "name": "@request-kit/preset-react-redux-entities",
  "version": "0.0.0",
  "description": "react high order component, bound with redux, to process request and provide data to component",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "build": "rollup -c --environment PACKAGE_NAME:$npm_package_name",
    "build:typings": "echo \"skip typings\"",
    "watch": "rollup -c -w --environment PACKAGE_NAME:$npm_package_name",
    "prepare": "npm run clean && npm run build:typings && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gleb-lobastov/request-kit.git"
  },
  "keywords": [
    "redux",
    "request",
    "data",
    "state"
  ],
  "author": "Gleb Lobastov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gleb-lobastov/request-kit/issues"
  },
  "homepage": "https://github.com/gleb-lobastov/request-kit#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jest-enzyme": "^7.1.0",
    "redux": "^4.0.0"
  },
  "dependencies": {
    "distribute-reducer": "^1.0.0",
    "@request-kit/request-default": "^0.0.1",
    "@request-kit/util-multiple-request": "^0.0.1",
    "@request-kit/intg-react-redux": "^0.0.0",
    "@request-kit/intg-redux-model-normalized": "^0.0.0",
    "@request-kit/provision-react": "^0.3.0"
  }
}
