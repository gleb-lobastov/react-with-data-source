{
  "name": "@request-kit/distribute-reducer",
  "version": "0.2.0",
  "description": "alter existent reducer to make hierarchical groups inside redux state",
  "author": "Gleb Lobastov",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "build": "rollup -c --environment PACKAGE_NAME:$npm_package_name",
