{
  "name": "@request-kit/model-normalized",
  "version": "0.0.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "build": "rollup -c --environment PACKAGE_NAME:$npm_package_name",
